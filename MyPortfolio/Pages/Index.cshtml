<script>
/* ----------- Unified modal + slideshow logic with auto-rotation ----------- */

const slideshows = {};
const autoSlideIntervals = {};

// Show the slide for a given modal
function showSlidesForModal(modalId, index) {
  const modal = document.getElementById(modalId);
  if (!modal) return;

  const slides = modal.querySelectorAll(".slides");
  if (!slides.length) return;

  // wrap index
  if (index > slides.length) index = 1;
  if (index < 1) index = slides.length;

  slideshows[modalId] = index;

  slides.forEach((s, i) => {
    s.style.display = (i + 1 === index) ? "block" : "none";
  });
}

// Move slide manually
function plusSlides(n, modalId) {
  slideshows[modalId] = (slideshows[modalId] || 1) + n;
  showSlidesForModal(modalId, slideshows[modalId]);
}

// Open modal and start auto-slideshow
function openModal(modalId) {
  const modal = document.getElementById(modalId);
  if (!modal) return;

  modal.style.display = "flex";
  document.body.style.overflow = "hidden";

  slideshows[modalId] = 1;
  showSlidesForModal(modalId, slideshows[modalId]);

  // Start automatic slideshow (every 3s)
  autoSlideIntervals[modalId] = setInterval(() => {
    plusSlides(1, modalId);
  }, 1500);
}

// Close modal and stop slideshow
function closeModal(modalId) {
  const modal = document.getElementById(modalId);
  if (!modal) return;

  modal.style.display = "none";
  document.body.style.overflow = "auto";

  // Stop auto slideshow
  clearInterval(autoSlideIntervals[modalId]);
}

/* ---------- Event handlers ---------- */

// Prev / Next buttons
document.addEventListener("click", (e) => {
  const prev = e.target.closest(".prev");
  const next = e.target.closest(".next");

  if (prev) {
    const modal = prev.closest(".modal");
    plusSlides(-1, modal.id);
  }
  if (next) {
    const modal = next.closest(".modal");
    plusSlides(1, modal.id);
  }
});

// Escape key closes modals
document.addEventListener("keydown", (e) => {
  if (e.key === "Escape") {
    document.querySelectorAll(".modal").forEach((modal) => {
      modal.style.display = "none";
      clearInterval(autoSlideIntervals[modal.id]);
    });
    document.body.style.overflow = "auto";
  }
});

// Hide all modals initially
window.addEventListener("load", () => {
  document.querySelectorAll(".modal").forEach((modal) => {
    modal.style.display = "none";
  });
});
</script>




